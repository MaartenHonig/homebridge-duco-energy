{
  "name": "homebridge-duco-energy",
  "version": "1.0.0",
  "description": "Homebridge plugin for Duco Energy ventilation systems with Connectivity Board 2.0",
  "main": "dist/index.js",
  “homebridge”: {
    “platforms”: [
    {
      “name”: “DucoEnergy”,
      “platform”: “DucoEnergy”
    }
    ]
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w"
  },
  "keywords": [
    "homebridge-plugin",
    "homebridge",
    "duco",
    "ventilation",
    "ducobox",
    "duco-energy",
    "humidity",
    "air-quality"
  ],
  "author": "Maarten",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaartenHonig/homebridge-duco-energy.git"
  },
  "engines": {
    "homebridge": ">=1.6.0",
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "sql.js": "^1.10.0",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "homebridge": "^1.6.0",
    "typescript": "^5.3.0"
  }
}
